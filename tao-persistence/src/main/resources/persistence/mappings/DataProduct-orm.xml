<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
                 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                 http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
                 version="2.0">
    <entity class="ro.cs.tao.eodata.EOProduct">
        <table name="tao.data_product"/>
        <attributes>
            <id name="automaticId">
                <column name="id" nullable="false"/>
                <generated-value strategy="SEQUENCE" generator="data_product_identifier"/>
                <sequence-generator name="data_product_identifier" sequence-name="tao.data_product_id_seq" allocation-size="1" />
            </id>
            <basic name="sensorType">
                <column name="sensor_type_id" nullable="false" column-definition="integer not null"/>
            </basic>

            <basic name="sensorType">
                <column name="sensor_type_id" nullable="false" column-definition="integer not null"/>
            </basic>
            <basic name="acquisitionDate">
                <column name="sensor_type_id" nullable="false" column-definition="timestamp"/>
            </basic>
            <basic name="pixelType">
                <column name="sensor_type_id" nullable="false" column-definition="integer not null"/>
            </basic>
            <basic name="productType">
                <column name="sensor_type_id" nullable="false" column-definition="integer not null"/>
            </basic>
            <basic name="width">
                <column name="sensor_type_id" nullable="false" column-definition="integer not null"/>
            </basic>
            <basic name="height">
                <column name="sensor_type_id" nullable="false" column-definition="integer not null"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>